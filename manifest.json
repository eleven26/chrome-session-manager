{
	"manifest_version": 2,

	"browser_action": {
		"default_icon"  : "logo/icon-48x48.png",
		"default_title" : "session manager",
		"default_popup" : "popup.html"
	},
	"description": "session manager",
	"icons": {
		"16"  : "logo/icon-16x16.png",
		"32"  : "logo/icon-32x32.png",
		"48"  : "logo/icon-48x48.png",
		"128"  : "logo/icon-128x128.png"
	},

	"web_accessible_resources": ["logo/icon-256x256.png"],
	"name": "session manager",
	"short_name": "session manager",
	"permissions": [
		"cookies",
		"http://*/*",
		"https://*/*",
    	"storage",
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"version": "1.0"
}
